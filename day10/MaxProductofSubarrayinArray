public class MaxProduct {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a= {6,-3,-10,0,2};
		int pro=1;
		int maxpro=Integer.MIN_VALUE;
		for(int i=0;i<a.length;i++)
		{
			pro*=a[i];
			if(pro>maxpro)
			{
				maxpro=pro;
			}
			if(pro==0)
			{
				pro=1;
			}
		}
		System.out.println(maxpro);

	}

}
